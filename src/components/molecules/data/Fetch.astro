---
const { title = 'Rick and Morty' } = Astro.props
const res = await fetch('https://api.sampleapis.com/rickandmorty/characters')
const data = await res.json()
---
<h1>{ title }</h1>

<div>
  {data.map(item => (
    <div>
      <h2>{item.name}</h2>
      <p>{item.status}</p>
      <p>{item.species}</p>
      <p>{item.origin}</p>
    </div>
  ))}
</div>